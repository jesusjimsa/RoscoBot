		<!-- Regla  -->
		<category>
			<pattern>[No Negativo Nope] ^</pattern>
			<that>SE VA A BORRAR EL EVENTO DEL *</that>
			<template>
				Entonces no borro nada
			</template>
		</category>
	</topic>
