	<topic name="BorrarEvento">
		<!-- Regla  -->
		<category>
			<pattern>*</pattern>
			<that>* DIME ^ LA FECHA DEL *</that>
			<template>
				<think>
					<set name="borrarFecha"><star/></set>
					<set var="comprobacionFecha"><srai>validdate <star/></srai></set>
